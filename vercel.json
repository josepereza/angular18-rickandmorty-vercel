

  {
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "builds": [
        {
          "src": "dist/angular18-rickandmorty-2/server/server.mjs",
          "use": "@vercel/node"
        }
      ],
    "public": true,
    "name": "ssr-angular",
    "rewrites": [{
        "source": "/(.*)",
        "destination": "/api"
      }
    ],
    "functions": {
      "api/server.mjs": {
        "includeFiles": "dist/angular18-rickandmorty-2/server/**"
      }
    }
  }