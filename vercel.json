

  {
    "version": 2,
    "builds": [
    {
      "src": "server.mjs",
      "use": "@vercel/node",
      "config": { "includeFiles": ["*"] }
    }
],
    "buildCommand": "npm run build",
    "installCommand": "npm install",
   
    "public": true,
    "name": "ssr-angular",
    "devCommand": "node dist/angular18-rickandmorty-2/server/server.mjs",
    "rewrites": [{
        "source": "/(.*)",
        "destination": "/api"
      }
    ],
    "functions": {
      "api/server.mjs": {
        "includeFiles": "dist/angular18-rickandmorty-2/browser/**"
      }
    }
  }